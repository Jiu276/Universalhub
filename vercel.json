{
  "builds": [
    {
      "src": "universalhub-site/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "universalhub-site/dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

